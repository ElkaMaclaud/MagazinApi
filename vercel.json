{
  "version": 2,
  "builds": [
    {
      "src": "build/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api(.*)", "dest": "build/main.js",
  "methods": [
    "GET",
    "POST",
    "PUT",
    "DELETE"
  ] }
  ],
    
  "env": {
    "PORT": "3000"
  },
  "outputDirectory": "dist" 
}


