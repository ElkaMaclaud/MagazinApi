{
  "version": 2,
    "build": {
      "commands": {
        "install": "npm install --force"
      }
  }, 
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/main.js"
    }
  ],
  "env": {
    "PORT": "5000"
  }
}
